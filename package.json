{
    "name": "pprof-it",
    "version": "1.0.1",
    "description": "Handy wrapper for pprof",
    "repository": "https://github.com/jakebailey/pprof-it",
    "author": "Jake Bailey",
    "license": "MIT",
    "engines": {
        "node": ">=12.0.0"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "LICENSE.txt"
    ],
    "scripts": {
        "clean": "rimraf dist",
        "prepack": "yarn run clean && tsc",
        "check": "yarn run check:eslint && yarn run check:prettier",
        "check:eslint": "eslint .",
        "fix:eslint": "eslint --fix .",
        "check:prettier": "prettier -c .",
        "fix:prettier": "prettier --write .",
        "release": "release-it"
    },
    "dependencies": {
        "@datadog/pprof": "^1.0.2",
        "signal-exit": "^3.0.7"
    },
    "devDependencies": {
        "@tsconfig/node12": "^1.0.11",
        "@types/node": "^12.20.55",
        "@types/signal-exit": "^3.0.1",
        "@typescript-eslint/eslint-plugin": "^5.39.0",
        "@typescript-eslint/parser": "^5.39.0",
        "eslint": "^8.24.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-simple-import-sort": "^8.0.0",
        "prettier": "2.7.1",
        "release-it": "^15.5.0",
        "rimraf": "^3.0.2",
        "typescript": "^4.8.4"
    },
    "packageManager": "npm@8.17.0",
    "volta": {
        "node": "14.20.0",
        "npm": "8.17.0"
    },
    "release-it": {
        "git": {
            "commitMessage": "Release v${version}",
            "tagName": "v${version}"
        }
    }
}
