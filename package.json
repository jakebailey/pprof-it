{
    "name": "pprof-it",
    "version": "0.1.1",
    "description": "Handy wrapper for pprof",
    "repository": "https://github.com/jakebailey/pprof-it",
    "author": "Jake Bailey",
    "license": "MIT",
    "engines": {
        "node": ">=12.0.0"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "LICENSE.txt"
    ],
    "scripts": {
        "clean": "rimraf dist",
        "prepack": "yarn run clean && tsc",
        "check": "yarn run check:eslint && yarn run check:prettier",
        "check:eslint": "eslint .",
        "fix:eslint": "eslint --fix .",
        "check:prettier": "prettier -c .",
        "fix:prettier": "prettier --write .",
        "release": "release-it"
    },
    "dependencies": {
        "@datadog/pprof": "^0.5.1",
        "signal-exit": "^3.0.7"
    },
    "devDependencies": {
        "@tsconfig/node12": "^1.0.11",
        "@types/node": "^12.20.55",
        "@types/signal-exit": "^3.0.1",
        "@typescript-eslint/eslint-plugin": "^5.29.0",
        "@typescript-eslint/parser": "^5.29.0",
        "eslint": "^8.18.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "prettier": "2.7.1",
        "release-it": "^15.1.0",
        "rimraf": "^3.0.2",
        "typescript": "^4.7.4"
    },
    "release-it": {
        "git": {
            "commitMessage": "Release v${version}",
            "tagName": "v${version}"
        }
    },
    "packageManager": "yarn@3.2.1"
}
