{
    "name": "pprof-it",
    "version": "1.2.1",
    "description": "Handy wrapper for pprof",
    "repository": "https://github.com/jakebailey/pprof-it",
    "author": "Jake Bailey",
    "license": "MIT",
    "engines": {
        "node": ">=12.0.0"
    },
    "bin": "dist/main.js",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "LICENSE.txt"
    ],
    "scripts": {
        "build": "tsc",
        "clean": "rimraf dist",
        "prepack": "npm run clean && npm run build",
        "check": "npm run check:eslint && npm run check:prettier",
        "check:eslint": "eslint .",
        "fix:eslint": "eslint --fix .",
        "check:prettier": "prettier -c .",
        "fix:prettier": "prettier --write .",
        "release": "release-it"
    },
    "dependencies": {
        "@datadog/pprof": "^2.1.0",
        "foreground-child": "^2.0.0",
        "pprof-format": "^2.0.7",
        "signal-exit": "^3.0.7"
    },
    "devDependencies": {
        "@tsconfig/node12": "^1.0.11",
        "@types/foreground-child": "^2.0.0",
        "@types/node": "^18.15.5",
        "@types/signal-exit": "^3.0.1",
        "@typescript-eslint/eslint-plugin": "^5.56.0",
        "@typescript-eslint/parser": "^5.56.0",
        "eslint": "^8.36.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-simple-import-sort": "^10.0.0",
        "prettier": "2.8.6",
        "release-it": "^15.9.1",
        "rimraf": "^4.4.0",
        "typescript": "^5.0.2"
    },
    "packageManager": "npm@8.17.0",
    "volta": {
        "node": "14.20.0",
        "npm": "8.17.0"
    },
    "release-it": {
        "git": {
            "commitMessage": "Release v${version}",
            "tagName": "v${version}"
        }
    }
}
